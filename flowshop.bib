@Article{Palmer65,
  Title                    = {Sequencing Jobs Through a Multi-Stage Process in the Minimum Total Time---A Quick Method of Obtaining a Near Optimum},
  Author                   = {Palmer, D. S.},
  Journal                  = {Journal of the Operational Research Society},
  Year                     = {1965},
